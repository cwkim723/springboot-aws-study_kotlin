{{>layout/header}}

<div class="col-md-12">
    <!-- 목록 출력 영역 -->
    <table class="table table-horizontal table-bordered">
        <thead class="thead-strong">
        <tr>
            <th>게시글번호</th>
            <th>제목</th>
            <th>작가</th>
            <th>대출 가능 권수</th>
        </tr>
        </thead>
        <tbody id="tbody">
        {{#books}}
            <tr>
                <td>{{id}}</td>
                <td><a href="/posts/update/{{id}}">{{title}}</a></td>
                <td>{{author}}</td>
                <td>{{stockQuantity}}</td>
            </tr>
        {{/books}}
        </tbody>
    </table>
</div>

{{>layout/footer}}